<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="497.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java_proj9_javafx_gui.Controller">
   <children>
      <Button fx:id="startButton" layoutX="267.0" layoutY="311.0" mnemonicParsing="false" onAction="#startPressed" text="Start!">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="playerName" layoutX="226.0" layoutY="179.0" onAction="#startPressed" promptText="Player Name" />
      <Text fx:id="askingNameLabel" layoutX="172.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type your Name and press Start!">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
